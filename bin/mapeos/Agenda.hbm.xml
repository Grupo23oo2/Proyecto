<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "../hibernate-mapping-3.0.dtd">
<hibernate-mapping package="datos">
    <class name="Agenda" table="agenda">
    
        <id name="idAgenda" column="idAgenda">
            <generator class="identity"/>
        </id>
        
        <property name="fechaHora" column="fechaHora" type="java.time.LocalDateTime"/>
        
        <set name="turnos" cascade="all-delete-orphan" inverse="true">
            <key column="idAgenda"/>
            <one-to-many class="datos.Turno"/>
        </set>
        
    </class>
</hibernate-mapping>