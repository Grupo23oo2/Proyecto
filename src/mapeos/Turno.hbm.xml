<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM "../hibernate-mapping-3.0.dtd">
<hibernate-mapping package="datos">
    <class name="Turno" table="turno">
        <id name="idTurno" column="idTurno">
            <generator class="increment"/>
        </id>
        <property name="presencial"/>
        <property name="fechaHora" column="fechaHora"/>
        <property name="estadoTurno" column="estadoTurno"/>

        <many-to-one name="lugarTurno" column="idLugar" class="Lugar"/>
        <many-to-one name="empleado" column="idEmpleado" class="Empleado"/>
        <many-to-one name="agenda" column="idAgenda" class="Agenda"/>
    </class>
</hibernate-mapping>